#include<stdio.h>
int main()
   {
    int a,b;
    printf("enter 1st number:\n");
    scanf("%d",&a);
    printf ("enter 2nd number:\n");
    scanf("%d",&b);
    if(a>b)
        {printf("%d number is greatest",a);
        }
    else {
        printf("%d number is greatest",b);
         }
    return 0;
    }
